<li><%= link_to 'Home', root_path, method: :get %></li>
<% if user_signed_in? %>
    <li><%= link_to 'Users', users_path, method: :get if current_user.admin? %></li>
    <li><%= link_to 'Dashboard', dashboard_path(current_user), method: :get %></li>
    <li><%= link_to 'Edit account', edit_user_registration_path, method: :get %></li>
    <li><%= link_to 'Sign out', destroy_user_session_path, :method => 'delete' %></li>
<% else %>
    <li><%= link_to 'Sign in', new_user_session_path, method: :get %></li>
    <li><%= link_to 'Sign up', new_user_registration_path, method: :get %></li>
<% end %>
